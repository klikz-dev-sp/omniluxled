tab == 'hydrocolloid-refill-patch-2' or 
tab == 'eye-brightener' or 
tab == 'hydrocolloid-refill-patch' or 
tab == 'skin-corrector' or 
tab == 'hydrocolloid-refill-patch-3'
-%}
