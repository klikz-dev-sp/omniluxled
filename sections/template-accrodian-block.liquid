                                                        {%- if section.settings.clear_image2.width >= 360 -%}{{ section.settings.clear_image2 | image_url: width: 360, format: 'pjpg' }} 360w,{%- endif -%}
                                                        {%- if section.settings.clear_image2.width >= 520 -%}{{ section.settings.clear_image2 | image_url: width: 520, format: 'pjpg' }} 520w,{%- endif -%}
                                                        {%- if section.settings.clear_image2.width >= 650 -%}{{ section.settings.clear_image2 | image_url: width: 650, format: 'pjpg' }} 650w,{%- endif -%}
                                                        {%- if section.settings.clear_image2.width >= 780 -%}{{ section.settings.clear_image2 | image_url: width: 780, format: 'pjpg' }} 780w,{%- endif -%}
                                                        {%- if section.settings.clear_image2.width >= 900 -%}{{ section.settings.clear_image2 | image_url: width: 900, format: 'pjpg' }} 900w,{%- endif -%}
                                                        {%- if section.settings.clear_image2.width >= 1000 -%}{{ section.settings.clear_image2 | image_url: width: 1000, format: 'pjpg' }} 1000w,{%- endif -%}"
                                                        src="{{ section.settings.clear_image2 | image_url: width: 20, format: 'pjpg' }}"
                                                        sizes="(min-width: {{ settings.container_width }}px) {{ settings.container_width | divided_by: 2 }}px, (min-width: 750px) calc((100vw - 130px) / 2), 100vw"
                                                        alt="{{ section.settings.image.alt | escape }}"
                                                        class="w-100 h-100"
                                                        loading="lazy"
                                                        width="{{ section.settings.image.width | divided_by: 3 }}"
                                                        height="{{ section.settings.image.height | divided_by: 3 }}"
                                                    >
                                                {%- else -%}
                                                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                                {%- endif -%}

                                            </div>
                                            <div class="py-3 px-4">
                                                {% if section.settings.Clear_image_text2 != blank %}
                                                            {{ section.settings.Clear_image_text2 }}
                                                    {% endif %}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                </div>
            </custom-accordion>
        </div>
    </div>
</section>


