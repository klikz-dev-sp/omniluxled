  | last
  | split: '"'
  | first
  | split: '.omniluxled.com'
  | last
  | replace: '\/', '/'
  | replace: '%20', ' '
  | replace: '\u0026', '&'
-%}
{%- assign tab = pageUrl | split:'?' | last | replace: 't=', '' -%}
